{
  "request": {
    "method": "POST",
    "urlPath": "/Api/v1/multi-exec-org/SigningRequests",
    "multipartPatterns": [
      {
        "matchingType": "ANY",
        "headers": {
          "Content-Disposition": {
            "contains": "name=\"SigningPolicySlug\""
          }
        },
        "bodyPatterns": [
          {
            "equalTo": "exe-policy"
          }
        ]
      }
    ]
  },
  "response": {
    "status": 201,
    "headers": {
      "Location": "{{request.baseUrl}}/Api/v1/multi-exec-org/SigningRequests/exe-req-id"
    },
    "transformers": ["response-template"]
  }
}
